{
    "name": "vue-test-utils-getting-started",
    "version": "1.0.0",
    "scripts": {
        "test": "jest"
    },
    "dependencies": {
        "axios": "^0.21.1",
        "vue": "^2.4.4"
    },
    "devDependencies": {
        "@vue/test-utils": "^1.0.0-beta.13",
        "babel-jest": "^21.2.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-env": "^1.6.0",
        "jest": "^21.2.1",
        "regenerator-runtime": "^0.11.0",
        "sinon": "^7.5.0",
        "vue-jest": "^3.0.5",
        "vue-template-compiler": "^2.4.4"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "vue"
        ],
        "transform": {
            ".*\\.(vue)$": "vue-jest",
            "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
        },
        "moduleNameMapper": {
            "^@/(.*)$": "<rootDir>/src/$1"
        },
        "collectCoverage": false,
        "coverageThreshold": {
            "global": {
                "branches": 90,
                "functions": 90,
                "lines": 90,
                "statements": 90
            },
            "./view/*.vue": {
                "branches": 40,
                "statements": 40
            }
        }
    },
    "author": "Evan You",
    "license": "MIT"
}